def print $obj {
    # Emits raw bytecode
    # Non-instruction items are compiled normally
    <obj, PRINT_LINE>
}

print "Hello, world"


# An argument can be added to an instruction like this:
#
#   <1, 2, 3, MAKE_ARRAY 3>
#
# which will make the array: [1, 2, 3]


# Emissions are expressions:
#
#   a = <2, 4, 6, MAKE_ARRAY 3>
#
# which sets a to [1, 2, 3]